@page
@using RazorRestaurant.Core
@model RazorRestaurant.Pages.Restaurants.List

@{
}

<h1>Restaurants</h1>

<table class="table">
    @foreach(var restaurant in Model.Restaurants)
    {
        <tr>
            <td>@restaurant.Name</td>
            <td>@restaurant.Location</td>
            <td>@restaurant.Cuisine</td>
        </tr>
    }
</table>
